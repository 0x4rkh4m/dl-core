{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "target": "esnext",
    "loose": true,
    "externalHelpers": true,
    "keepClassNames": true
  },
  "env": {
    "targets": {
      "node": "current"
    },
    "mode": "entry",
    "coreJs": 3
  },
  "minify": true
}